    <behaviors>
      <serviceBehaviors>
        <behavior name="PubsServiceBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="Pubs.Services.PubsService" behaviorConfiguration="PubsServiceBehavior">
        <endpoint address="" binding="basicHttpBinding" contract="Pubs.Services.Contracts.IPubsService" />
